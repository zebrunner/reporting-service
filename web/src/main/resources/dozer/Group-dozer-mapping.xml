		<field>
			<a>name</a>
			<b>name</b>
		</field>
		<field>
			<a>role</a>
			<b>role</b>
		</field>
		<field>
			<a>invitable</a>
			<b>invitable</b>
		</field>
		<field>
			<a>users</a>
			<b>users</b>
		</field>
		<field>
			<a>permissions</a>
			<b>permissions</b>
		</field>
	</mapping>
</mappings>
