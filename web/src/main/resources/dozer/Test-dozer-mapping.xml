		<field>
			<a>needRerun</a>
			<b>needRerun</b>
		</field>
		<field>
			<a>dependsOnMethods</a>
			<b>dependsOnMethods</b>
		</field>
		<field-exclude>
			<a>workItems</a>
			<b>workItems</b>
		</field-exclude>
		<field>
			<a>testClass</a>
			<b>testClass</b>
		</field>
		<field>
			<a>artifacts</a>
			<b>artifacts</b>
		</field>
		<field>
			<a>ciTestId</a>
			<b>ciTestId</b>
		</field>
		<field>
			<a>tags</a>
			<b>tags</b>
		</field>
