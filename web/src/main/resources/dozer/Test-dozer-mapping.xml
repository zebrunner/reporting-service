	xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<mapping type="bi-directional" wildcard="false">
		<class-a>com.zebrunner.reporting.domain.dto.TestType</class-a>
		<class-b>com.zebrunner.reporting.domain.db.Test</class-b>

		<field>
			<a>id</a>
			<b>id</b>
		</field>
		<field>
			<a>name</a>
			<b>name</b>
		</field>
		<field>
			<a>status</a>
			<b>status</b>
		</field>
		<field>
			<a>testArgs</a>
			<b>testArgs</b>
		</field>
		<field>
			<a>testRunId</a>
			<b>testRunId</b>
		</field>
		<field>
			<a>testCaseId</a>
			<b>testCaseId</b>
		</field>
		<field>
			<a>testGroup</a>
			<b>testGroup</b>
		</field>
		<field>
			<a>message</a>
			<b>message</b>
		</field>
		<field>
			<a>messageHashCode</a>
			<b>messageHashCode</b>
		</field>
		<field>
			<a>startTime</a>
			<b>startTime</b>
		</field>
		<field>
			<a>finishTime</a>
			<b>finishTime</b>
		</field>

		<field>
			<a>retry</a>
			<b>retry</b>
		</field>
		<field>
			<a>knownIssue</a>
			<b>knownIssue</b>
		</field>
		<field>
			<a>blocker</a>
			<b>blocker</b>
