<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <mapping type="bi-directional" wildcard="false">
        <class-a>com.zebrunner.reporting.domain.dto.integration.IntegrationDTO</class-a>
        <class-b>com.zebrunner.reporting.domain.entity.integration.Integration</class-b>

        <field>
            <a>id</a>
            <b>id</b>
        </field>
        <field>
            <a>name</a>
            <b>name</b>
        </field>
        <field>
            <a>backReferenceId</a>
            <b>backReferenceId</b>
        </field>
        <field>
            <a get-method="isDefault" set-method="setDefault">isDefault</a>
            <b get-method="isDefault" set-method="setDefault">isDefault</b>
        </field>
        <field>
            <a>enabled</a>
            <b>enabled</b>
        </field>
        <field>
            <a>settings</a>
            <b>settings</b>
        </field>
        <field>
            <a>type</a>
            <b>type</b>
        </field>
    </mapping>
</mappings>
